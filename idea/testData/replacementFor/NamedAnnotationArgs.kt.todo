// WITH_RUNTIME
import javax.swing.SwingUtilities

@ReplacementFor(imports = arrayOf("javax.swing.SwingUtilities"), expressions = *arrayOf("SwingUtilities.invokeLater(this)"))
fun Runnable.invokeItLater() {
    SwingUtilities.invokeLater(this)
}

fun foo() {
    SwingUtilities.invokeLater(Runnable {
        print(1)
    })
}